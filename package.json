{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --port 3000",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "typecheck": "tsc --noEmit",
        "lint": "eslint --ext .tsx,.ts,.jsx,.js src/",
        "lint:fix": "npm run lint -- --fix",
        "test": "npm run lint && npm run typecheck",
        "prettier:test": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
        "prettier:write": "prettier --write \"**/*.{js,jsx,ts,tsx}\""
    },
    "dependencies": {
        "@emotion/react": "^11.10.5",
        "@mantine/core": "^5.7.0",
        "@mantine/dates": "^5.7.0",
        "@mantine/form": "^5.7.0",
        "@mantine/hooks": "^5.7.0",
        "@mantine/modals": "^5.7.0",
        "@mantine/notifications": "^5.7.0",
        "@tabler/icons": "^1.110.0",
        "@tanstack/react-query": "^4.14.3",
        "@tanstack/react-query-devtools": "^4.14.3",
        "dayjs": "^1.11.6",
        "moment": "^2.29.4",
        "path": "^0.12.7",
        "react": "^18.2.0",
        "react-big-calendar": "^1.5.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.4.3"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.19.6",
        "@types/react": "^18.0.25",
        "@types/react-big-calendar": "^0.38.2",
        "@types/react-dom": "^18.0.8",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "@vitejs/plugin-react": "^2.2.0",
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "prettier": "^2.7.1",
        "typescript": "^4.8.4",
        "vite": "^3.2.2"
    }
}